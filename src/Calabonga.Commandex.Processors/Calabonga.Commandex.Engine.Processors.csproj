<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>Calabonga.Commandex.Engine.Processors</Title>
		<PackageId>Calabonga.Commandex.Engine.Processors</PackageId>
		<Version>1.0.0-beta.1</Version>
		<Authors>Calabonga</Authors>
		<Company>Calabonga SOFT</Company>
		<Description>Results Processors for Calabonga.Commandex commands execution results. This is an extended version of the just show string in the notification dialog.</Description>
		<Copyright>Calabonga SOFT Â© 2001 - $([System.DateTime]::Now.ToString(yyyy))</Copyright>
		<PackageTags>commandex;calabonga;wpf;appdefinitions;engine;core;plugins;mvvm;communitytoolkit;processors</PackageTags>
		<PackageReleaseNotes>First beta released</PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/Calabonga/Calabonga.Commandex.Engine.Processors</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Calabonga/Calabonga.Commandex.Engine.Processors</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageIcon>logo.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Calabonga.Commandex.Engine" Version="1.2.0-beta.1" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
	  <None Update="logo.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
